ALGORITHM insertion_sort
VAR
    courant,i,j: INTEGER;
    arr : ARRAY_OF integer;
BEGIN
    FOR i FROM 1 TO arr.length DO
        courant:=arr[i];
        j:=i-1;
        while (j>=0) and arr[j]>courant
            arr[j+1]:=arr[j]  
        j:=j+1;
        END_WHILE
        arr[j+1]:=courant
    END_FOR
END